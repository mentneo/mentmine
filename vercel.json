{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/manifest.json", "dest": "/manifest.json" },
    { "src": "/favicon.ico", "dest": "/favicon.ico" },
    { "src": "/logo-3d.png", "dest": "/logo-3d.png" },
    { "src": "/robots.txt", "dest": "/robots.txt" },
    { "src": "/sitemap.xml", "dest": "/sitemap.xml" },
    
    
    { "src": "^/about/?$", "dest": "/index.html" },
    { "src": "^/contact/?$", "dest": "/index.html" },
    { "src": "^/for-students/?$", "dest": "/index.html" },
    { "src": "^/hire-with-us/?$", "dest": "/index.html" },
    { "src": "^/privacy/?$", "dest": "/index.html" },
    { "src": "^/terms/?$", "dest": "/index.html" },
    { "src": "^/courses/?$", "dest": "/index.html" },
    { "src": "^/courses/[^/]+/?$", "dest": "/index.html" },
    { "src": "^/events/?$", "dest": "/index.html" },
    { "src": "^/events/[^/]+/?$", "dest": "/index.html" },
    { "src": "^/admin/?$", "dest": "/index.html" },
    { "src": "^/admin/login/?$", "dest": "/index.html" },
    { "src": "^/admin/dashboard/?$", "dest": "/index.html" },
    { "src": "^/admin/courses/?$", "dest": "/index.html" },
    { "src": "^/admin/users/?$", "dest": "/index.html" },
    { "src": "^/admin/events/?$", "dest": "/index.html" },
    { "src": "^/admin/settings/?$", "dest": "/index.html" },
    
    
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
